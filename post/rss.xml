&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="nv">jar&lt;/span> &lt;span class="nv">-xvf&lt;/span> &lt;span class="nv">xxx.jar&lt;/span>&lt;/span>&lt;/span>&lt;/code>&lt;/pre>&lt;/td>&lt;/tr>&lt;/table>
&lt;/div>
